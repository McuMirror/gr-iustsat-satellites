<?xml version="1.0"?>
<block>
  <name>Check CC11xx CRC-16</name>
  <key>satellites_check_cc11xx_crc</key>
  <category>[Satellites]/CC11xx</category>
  <import>import satellites</import>
  <make>satellites.check_cc11xx_crc($verbose)</make>
  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <type>bool</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>ok</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  <source>
    <name>fail</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
